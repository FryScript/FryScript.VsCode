{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Fry Script",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#operators"
		},
		{
			"include": "#strings"
		},
		{
			"include": "#constants"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [
				{
					"name": "keyword.control.fryscript",
					"match": "\\b(if|else|while|for|foreach|return|yield|await|break|continue|in|try|catch|finally|throw|out)\\b"
				},
				{
					"name": "keyword.fryscript",
					"match": "(^@\\b(import|extend|proto)\\b)"
				},
				{
					"name": "keyword.fryscript",
					"match": "\\b(var|fibre|as|extend|extends|has|params|base|this|new)\\b"
				}
			]
		},
		"operators": {
			"patterns": [
				{
					"name": "keyword.operator.fryscript",
					"match": "(&&|\\|\\||==|!=|>=|<=|\\+=|-=|\\*=|\\/=|\\%=|\\?=|=>)"
				},
				{
					"name": "keyword.operator.fryscript",
					"match": "(\\+|-|\\*|\\/|\\%|=|!|\\?|:)"
				},
				{
					"name": "support.constant.fryscript",
					"match": "(\\.|,|;)"
				}
			]
		},
		"constants": {
			"patterns": [
				{
					"name": "constant.language.fryscript",
					"match": "\\b(true|false|null)\\b"
				},
				{
					"name": "constant.numeric.fryscript",
					"match": "\\d"
				}
			]
		},
		"strings": {
			"name": "string.quoted.double.fryscript",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.fryscript",
					"match": "\\\\."
				}
			]
		}
	},
	"scopeName": "source.fry"
}